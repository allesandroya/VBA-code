Selection.AutoFill Destination:=Range("I4:I" & lastRow), Type:=xlFillDefault

ActiveSheet.Range("$A$3:$L$682").AutoFilter Field:=7, Criteria1:="<>"
ActiveSheet.Range("$A$3:$L$682").AutoFilter Field:=8, Criteria1:="#N/A"
ActiveSheet.Range("$A$3:$L$682").AutoFilter Field:=9, Criteria1:="#N/A"

